{
  "name": "react-redux-autoconnect",
  "version": "1.0.0",
  "description": "React Redux Autoconnect",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/ test/",
    "test": "shx rm -Rf results && cross-env NODE_ENV=test nyc mocha --timeout 10000 --colors --recursive --compilersjs:babel-register src/**/*.spec.js",
    "coverage": "npm run test && npm run coveralls",
    "coveralls": "shx cat ./results/coverage/lcov.info | coveralls",
    "build": "shx rm -Rf dist .tmp && cpx \"src/**/!(*.spec.js)\" .tmp && babel .tmp/ -d dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oxyno-zeta/react-redux-autoconnect.git"
  },
  "author": "Oxyno-zeta",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/oxyno-zeta/react-redux-autoconnect/issues"
  },
  "homepage": "https://github.com/oxyno-zeta/react-redux-autoconnect#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.23.0",
    "chai": "^4.0.2",
    "concurrently": "^3.5.0",
    "coveralls": "^2.13.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.0.1",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.2",
    "react": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "shx": "^0.2.2",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0"
  },
  "peerDependencies": {
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  }
}
